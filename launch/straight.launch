<?xml version="1.0"?>
<launch>
   
  <arg name="x_pose" default="1.55"/>
  <arg name="y_pose" default="0"/>
  <arg name="z_pose" default="0.0"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find project_2)/worlds/straight.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>  


  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find project_2)/robot_description/turtlebot3_waffle.urdf.xacro" />
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model project_2 -x $(arg x_pose) -y $(arg y_pose) -z $(arg z_pose)" />

</launch>

